<div class="col-md-6">
  <%= bootstrap_form_for [@user, @address] do |f| %>
    <p>
      <%= f.text_field :street_address  %>
    </p>
    <p>
      <%= f.text_field :street_address2 %>
    </p>
    <p>
      <%= f.text_field :city  %>
    </p>
    <p>
      <%= f.text_field :state  %>
    </p>
    <p>
      <%= f.text_field :zip_code %>
    </p>
    <%= f.primary %>
  <% end %>
</div>
